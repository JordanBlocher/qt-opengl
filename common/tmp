bool GLNode::Add(std::unique_ptr<GLNode> node)
{
    if( QGLApp::QGLNode.count(node->name) == 0 )
    {
        QGLApp::QGLNode.insert(QGLPair(node->name, std::move(node)));
        return true;
    }
    return false;
}

bool GLNode::Remove(const char* name)
{
    if( QGLApp::QGLNode.count(name) == 1 )
    {
        QGLApp::QGLNode.erase(name);
        return true;
    }
    return false;
}

